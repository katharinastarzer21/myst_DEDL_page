e'] = [15, 15]\ncrs = area_def.to_cartopy_crs()\nfig, ax = plt.subplots(subplot_kw=dict(projection=crs))\ncoastlines = ax.coastlines()  \nax.set_global()\n\n#plot\nimg = plt.imshow(result, transform=crs, extent=crs.bounds, origin='upper')\n# Calculate (height_of_image / width_of_image)\nim_ratio = result.shape[0]/result.shape[1]\n \n# Plot vertical colorbar\nplt.colorbar(fraction=0.047*im_ratio)\nplt.show()","visibility":"show","key":"mc4xMsLghI"},{"type":"output","id":"vqIrm9BokZeKjTHi2Mchs","data":[{"output_type":"display_data","metadata":{},"data":{"image/png":{"content_type":"image/png","hash":"ef0abf1e0655b71a8ddf11bf3873b72a","path":"/build/ef0abf1e0655b71a8ddf11bf3873b72a.png"},"text/plain":{"content":"<Figure size 1500x1500 with 2 Axes>","content_type":"text/plain"}}}],"visibility":"show","key":"BxVoKYk5eP"}],"visibility":"show","key":"fDUV3VGEwF"}],"key":"ySW51v4xq6"},"references":{"cite":{"order":[],"data":{}}},"footer":{"navigation":{"prev":{"title":"Destination Earth - AVHRR Level 1B Metop Global - Data Access using DEDL HDA","url":"/dedl-hda-eo-eum-dat-metop-avhrrl1","group":"HDA"},"next":{"title":"Example of Using HDA to Find and Download Data for Urban Area Monitoring Using Sentinel-1 Data","url":"/dedl-hda-eo-esa-dat-sentinel-1-l1-grd","group":"HDA"}}},"domain":"http://localhost:3001"}